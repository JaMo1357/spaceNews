    "vfluentdesign": "^0.3.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.7.1",
    "sass": "^1.83.0",
    "typescript": "~5.6.0",
    "vite": "^6.1.0",
    "vue-tsc": "^2.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
